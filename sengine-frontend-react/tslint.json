{
    "defaultSeverity": "error",
    "rulesDirectory": ["tslint-plugin-prettier"],
    "extends": [
        "tslint:recommended", "tslint-react", "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prefer-const": false,
        "prettier": true,
        "jsx-no=multiline-js": false,
        "jsx-no-lambda": false,
        "import-name": false,
        "no-boolean-literal-compare": false
    }
}