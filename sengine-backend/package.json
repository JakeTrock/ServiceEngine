{
  "name": "Sengine",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "instage":"cp .env ./dist && cp package*.json ./dist && cd dist && npm ci --only=production",
    "develop": "ts-node-dev index.ts",
    "dbuild": "npm run build && npm run instage && sudo docker build . -t sengine-backend",
    "drun": "sudo docker run -p 3000:3000 sengine-backend"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/mongoose": "^5.10.4",
    "@types/node": "^15.6.1",
    "@types/passport": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "lodash": "^4.17.20",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "tslint": "5.12.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "aws-sdk": "^2.824.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose-fuzzy-search": "^0.0.4",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "winston": "^3.3.3"
  }
}
