  "main": "index.js",
  "scripts": {
    "client": "cd sengine-frontend && yarn develop",
    "server": "cd sengine-backend && yarn develop",
    "prodClient": "cd sengine-frontend && serve -s build -l 3000",
    "prodServer": "cd sengine-backend && yarn build && yarn start",
    "develop": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "production": "concurrently --kill-others \"npm run prodServer\" \"npm run prodClient\""
